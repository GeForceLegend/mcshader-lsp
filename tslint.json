{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "semicolon": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "interface-name": false,
        "indent": [true, "spaces", 2],
        "max-classes-per-file": false,
        "no-console": false,
        "eofline": false,
        "member-ordering": false,
        "trailing-comma": false,
        "max-line-length": [true, 170],
        "radix": false,
        "whitespace": [
            true,
            "check-decl",
            "check-operator",
            "check-module",
            "check-rest-spread",
            "check-type",
            "check-typecast",
            "check-type-operator",
            "check-preblock"
        ]
    },
    "rulesDirectory": []
}